<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Debug">
        <Item>
          <IdeaActionID>ActivateDebugToolWindow</IdeaActionID>
          <ShortCut>'⌘5'</ShortCut>
          <Description>Debug</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'RecursiveMultiplicat...'">
        <Item>
          <IdeaActionID>DebugClass</IdeaActionID>
          <ShortCut>'⌃⇧F9'</ShortCut>
          <Description>Debug 'RecursiveMultiplicat...'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug...">
        <Item>
          <IdeaActionID>ChooseDebugConfiguration</IdeaActionID>
          <ShortCut>'⌥⇧F9'</ShortCut>
          <Description>Debug...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>32</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'⇧F6' or '⇧⌘R'</ShortCut>
          <Description>Rename...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'F9'</ShortCut>
          <Description>Resume Program</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'⌥⌘Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Generator: generate_...'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'⌃⇧F10'</ShortCut>
          <Description>Run 'Generator: generate_...'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show All Affected Files">
        <Item>
          <IdeaActionID>VcsHistory.ShowAllAffected</IdeaActionID>
          <ShortCut>'⌥⇧A'</ShortCut>
          <Description>Show All Affected Files</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Test">
        <Item>
          <IdeaActionID>GotoTest</IdeaActionID>
          <ShortCut>'⇧⌘T'</ShortCut>
          <Description>Test</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>